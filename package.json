{
  "name": "@adalo/eslint",
  "repository": "https://github.com/AdaloHQ/eslint",
  "author": "Toby Sullivan <toby.sullivan@adalo.com>",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --build",
    "clean": "tsc --build --clean",
    "test:eslint-plugin-axios": "cd packages/eslint-plugin-axios && yarn test",
    "prettier": "prettier --write './**'",
    "prettier:check": "prettier --check './**'",
    "lint": "eslint --fix --ext .js,.ts",
    "lint:check": "eslint --ext .js,.ts"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-eslint-plugin": "4.0.1",
    "eslint-plugin-node": "11.1.0",
    "prettier": "2.6.2",
    "typescript": "4.6.3"
  },
  "dependencies": {}
}
